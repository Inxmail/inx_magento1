{
  "name": "flagbit/magento-inxmail",
  "license": "proprietary",
  "type": "magento-module",
  "description": "Inxmail Magento Module",
  "require": {
    "magento-hackathon/magento-composer-installer": "*",
    "php": ">=5.2.0"
  },
  "require-dev": {
  },
  "extra":{
    "map": [
      ["src/app/etc/modules/DndInxmail_Subscriber.xml", "app/etc/modules/DndInxmail_Subscriber.xml"],
      ["src/app/code/community/DndInxmail/*", "app/code/community/DndInxmail"],
      ["src/app/design/adminhtml/default/default/layout/dndinxmail.xml", "app/design/adminhtml/default/default/layout/dndinxmail.xml"],
      ["src/app/design/adminhtml/default/default/template/dndinxmail", "app/design/adminhtml/default/default/template/dndinxmail"],
      ["src/app/design/frontend/default/layout/dndinxmail.xml", "app/design/frontend/default/layout/dndinxmail.xml"],
      ["src/app/design/frontend/default/template/dndinxmail", "app/design/frontend/default/template/dndinxmail"],
      ["src/app/locale/en_US/DndInxmail_Subscriber.csv", "app/locale/en_US/DndInxmail_Subscriber.csv"],
      ["src/app/locale/de_DE/DndInxmail_Subscriber.csv", "app/locale/de_DE/DndInxmail_Subscriber.csv"],
      ["src/app/locale/fr_FR/DndInxmail_Subscriber.csv", "app/locale/fr_FR/DndInxmail_Subscriber.csv"],
      ["src/js/dndinxmail", "js/dndinxmail"],
      ["src/lib/Inx", "lib/Inx"],
      ["src/shell/dndinxmail", "shell/dndinxmail"],
      ["src/skin/frontend/base/default/dndinxmail", "skin/frontend/base/default/dndinxmail"]
    ]
  }

}
